 hBmpDC)
{
    HBITMAP RetBmp=3DNULL;
    <span class=3D"code-keyword">if</span> (hBmp)
    {=09
        HDC BufferDC=3DCreateCompatibleDC(NULL);	<span class=3D"code-commen=
t">//</span><span class=3D"code-comment"> DC for Source Bitmap</span>

	<span class=3D"code-keyword">if</span> (BufferDC)
	{
	    HBITMAP hTmpBitmap =3D (HBITMAP) NULL;
	    <span class=3D"code-keyword">if</span> (hBmpDC)
	        <span class=3D"code-keyword">if</span> (hBmp =3D=3D (HBITMAP)GetCu=
