		    SelectObject(hBmpDC, hTmpBitmap);
		}
	   =20
            HGDIOBJ PreviousBufferObject=3DSelectObject(BufferDC,hBmp);
	    <span class=3D"code-comment">//</span><span class=3D"code-comment"> he=
re BufferDC contains the bitmap</span>

		=09
	    HDC DirectDC=3DCreateCompatibleDC(NULL); <span class=3D"code-comment">=
//</span><span class=3D"code-comment"> DC for working</span>

	    <span class=3D"code-keyword">if</span> (DirectDC)
	    {
		<span class=3D"code-comment">//</span><span class=3D"code-comment"> Get b=
itmap size</span>

		BITMAP bm;
