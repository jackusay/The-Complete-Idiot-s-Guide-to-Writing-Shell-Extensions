		RGB32BitsBITMAPINFO.bmiHeader.biHeight=3Dbm.bmHeight;
		RGB32BitsBITMAPINFO.bmiHeader.biPlanes=3D<span class=3D"code-digit">1</sp=
an>;
		RGB32BitsBITMAPINFO.bmiHeader.biBitCount=3D<span class=3D"code-digit">32<=
/span>;

                <span class=3D"code-comment">//</span><span class=3D"code-c=
omment"> pointer used for direct Bitmap pixels access</span>

		UINT * ptPixels;=09

		HBITMAP DirectBitmap =3D CreateDIBSection(DirectDC,=20
                                       (BITMAPINFO *)&amp;RGB32BitsBITMAPIN=
FO,=20
                                       DIB_RGB_COLORS,
                                       (<span class=3D"code-keyword">void</=
span> **)&amp;ptPixels,=20
                                       NULL, <span class=3D"code-digit">0</=
span>);
		<span class=3D"code-keyword">if</span> (DirectBitmap)
		{
		    <span class=3D"code-comment">//</span><span class=3D"code-comment"> h=
